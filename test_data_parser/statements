proc statements()
{
	var a int
	var b int
	c := 100
	a = c
	a /= b
	return
	return foo

	{
		single_nested := true
		{
			double_nested := true
			single_nested = false
		}
	}
}
======
proc statements()
{
	var a int
	var b int
	c := 100
	a = c
	a = (a / b)
	return
	return foo
	{
		single_nested := true
		{
			double_nested := true
			single_nested = false
		}
	}
}
