proc statements()
{
	var a int
	var b int
	c := 100
	a = c
	a /= b
	return
	return foo

	{
		single_nested := true
		{
			double_nested := true
			single_nested = false
		}
	}

	if true {
		a += 10
	}

	condition := false
	if condition {
		a -= 4
		return a
	} else {
		b = a - b
		return b
	}

	i := 0
	for {
		if i > 10 {
			break
		}

		i += 1
	}
}
======
proc statements()
{
	var a int
	var b int
	c := 100
	a = c
	a = (a / b)
	return
	return foo
	{
		single_nested := true
		{
			double_nested := true
			single_nested = false
		}
	}
	if true {
		a = (a + 10)
	}
	condition := false
	if condition {
		a = (a - 4)
		return a
	} else {
		b = (a - b)
		return b
	}
	i := 0
	for {
		if (i > 10) {
			break
		}
		i = (i + 1)
	}
}
