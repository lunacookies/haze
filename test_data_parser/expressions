proc expressions()
{
	10 20
	abc
	true false

	a + b
	1 + 2 + 3 + 4
	1 + 2 * 3
	2 == 3 && 4 == 5 || 6 == 7 && 8 == 9
	2 & 3 | 6 & 7
	2 & 3 ^ 6 & 7
	1 << 2 + 2
	1 | 2 ^ 3 | 4 ^ 5

	proc_name()
	proc_name(a)
	proc_name(a, b)
	proc_name(1 + 1, 2 * 2)
	proc_name(trailing, comma,)
}
======
proc expressions()
{
	10
	20
	abc
	true
	false
	(a + b)
	(((1 + 2) + 3) + 4)
	(1 + (2 * 3))
	(((2 == 3) && (4 == 5)) || ((6 == 7) && (8 == 9)))
	((2 & 3) | (6 & 7))
	((2 & 3) ^ (6 & 7))
	((1 << 2) + 2)
	((((1 | 2) ^ 3) | 4) ^ 5)
	proc_name()
	proc_name(a)
	proc_name(a, b)
	proc_name((1 + 1), (2 * 2))
	proc_name(trailing, comma)
}
