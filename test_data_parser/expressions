func expressions() {
	10; 20
	abc
	true; false
	"hello"
	"\t\\textit{\"italics\"}\n"

	a + b
	1 + 2 + 3 + 4
	1 + 2 * 3
	2 == 3 && 4 == 5 || 6 == 7 && 8 == 9
	2 & 3 | 6 & 7
	2 & 3 ^ 6 & 7
	1 << 2 + 2
	1 | 2 ^ 3 | 4 ^ 5
	~1 - -1
	!true && false

	func_name()
	func_name(a)
	func_name(a, b)
	func_name(1 + 1, 2 * 2)
	func_name(trailing, comma,)

	var my_t MyType
	my_t.a = 92
	my_t.b.c = 29
	my_t.a = b.c

	x(&foo)
	x(&foo.bar.baz)
	x(*ptr)
	x(*ptr.with.fields)
	x(*a + 10)
	x(&a + 10)

	cast(int) true
	cast(int) a + 10

	foo[bar]
	1 + 2[3]
	a[b][c]
	a[b[c]]
}

struct MyType {
	a int
	b Nested
}

struct Nested {
	c int
}
======
func expressions()
{
	10
	20
	abc
	true
	false
	"hello"
	"\t\\textit{\"italics\"}\n"
	(a + b)
	(((1 + 2) + 3) + 4)
	(1 + (2 * 3))
	(((2 == 3) && (4 == 5)) || ((6 == 7) && (8 == 9)))
	((2 & 3) | (6 & 7))
	((2 & 3) ^ (6 & 7))
	((1 << 2) + 2)
	((((1 | 2) ^ 3) | 4) ^ 5)
	((~1) - (-1))
	((!true) && false)
	func_name()
	func_name(a)
	func_name(a, b)
	func_name((1 + 1), (2 * 2))
	func_name(trailing, comma)
	var my_t MyType
	(my_t.a) = 92
	((my_t.b).c) = 29
	(my_t.a) = (b.c)
	x((&foo))
	x((&((foo.bar).baz)))
	x((*ptr))
	x((*((ptr.with).fields)))
	x(((*a) + 10))
	x(((&a) + 10))
	cast(int, true)
	(cast(int, a) + 10)
	(foo[bar])
	(1 + (2[3]))
	((a[b])[c])
	(a[(b[c])])
}

struct MyType
{
	a int
	b Nested
}

struct Nested
{
	c int
}
