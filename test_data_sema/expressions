func expressions(parameter int, slice []int) {
	10
	true
	false
	"hello"
	a := 92
	a + 2 * 3 * 4 * 5
	~a - -1
	!true && false
	parameter
	b := add(9, 2)
	ptr := &b
	ptr_value := *ptr
	also_ptr := cast([*]int) ptr
	also_ptr_value := also_ptr[0]
	s := "a string"
	slice[8]
	slice_data := slice.data
	slice_count := slice.count
}

func add(x int, y int) int {
	return x + y
}
======
func expressions
	param parameter int
	param slice []int
	var a int
	var b int
	var ptr *int
	var ptr_value int
	var also_ptr [*]int
	var also_ptr_value int
	var s []byte
	var slice_data [*]int
	var slice_count int
{
	10
	true
	false
	"hello"
	a = 92
	(a + (((2 * 3) * 4) * 5))
	((~a) - (-1))
	((!true) && false)
	parameter
	b = add(9, 2)
	ptr = (&b)
	ptr_value = (*ptr)
	also_ptr = cast([*]int, ptr)
	also_ptr_value = (also_ptr[0])
	s = "a string"
	(slice[8])
	slice_data = slice.data
	slice_count = slice.count
}

func add
	param x int
	param y int
{
	return (x + y)
}
