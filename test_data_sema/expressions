proc expressions(parameter int)
{
	10
	true
	false
	a := 92
	a + 2 * 3 * 4 * 5
	parameter
	b := add(9, 2)
	ptr := &b
	ptr_value := *ptr
}

proc add(x int, y int) int
{
	return x + y
}
======
proc expressions
	param parameter int
	var a int
	var b int
	var ptr *int
	var ptr_value int
{
	10
	true
	false
	a = 92
	(a + (((2 * 3) * 4) * 5))
	parameter
	b = add(9, 2)
	ptr = (&b)
	ptr_value = (*ptr)
}

proc add
	param x int
	param y int
{
	return (x + y)
}
