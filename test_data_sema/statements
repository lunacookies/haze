struct Wrapper {
	value int
}

proc statements() Wrapper
{
	var w Wrapper
	var a int

	a = 5
	b := a

	{
		b -= 1
	}

	if true {
		a = b
	} else {
		b = a
	}

	for {
		if b > a * 2 {
			break
		}
		b += 2
	}

	no_return_value(4)

	return w
}

proc no_return_value(val int) {}
======
proc statements
	var w Wrapper
	var a int
	var b int
{
	a = 5
	b = a
	{
		b = (b - 1)
	}
	if true {
		a = b
	} else {
		b = a
	}
	for {
		if (b > (a * 2)) {
			break
		}
		b = (b + 2)
	}
	call no_return_value(4)
	return w
}

proc no_return_value
	var val int
{
}
